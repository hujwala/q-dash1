<div class="container">
  <div class="row">
    <div class="col-sm-3"></div>
    <div class="col-sm-6">
      <div class="row">
        <div class="col-sm-12" style="margin-top:200px;">
          <div id="div_flash_message" id="flash">
            <%= render :partial=>"/layouts/dashing/flash_messages" -%>
          </div>
          <div class="well">
            <h3>Log In</h3>
            <%= form_tag admin_sessions_path, :method=>:post do %>
            <div class="form-group">
              <label for="inputEvent" class="col-sm-3 control-label">Email * : </label>
              <div class="col-sm-7">
                <%= text_field_tag :email%>
              </div>
              <div class="col-sm-1"></div>
            </div><br><br>
            <div class="form-group">
              <label for="inputVenue" class="col-sm-3 control-label">Password * :</label>
              <div class="col-sm-7">
                <%= password_field_tag :password %>
              </div>
              <div class="col-sm-1"></div>
            </div><br><br>
            <div class="modal-footer">
              <%= submit_tag("OK", :class => "btn btn-primary") %>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>